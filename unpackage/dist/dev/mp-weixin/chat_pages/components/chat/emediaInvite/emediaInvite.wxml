<view class="wraper"><block wx:if="{{search_btn}}"><view class="search"><view bindtap="openSearch"><icon type="search" size="13"></icon><text>搜索</text></view></view></block><block wx:if="{{search_friend}}"><view class="search_input"><view><icon type="search" size="13"></icon><input placeholder="搜索" placeholder-style="color:#CFCFCF;line-height:20px;font-size:12px;" auto-focus="{{true}}" confirm-type="search" type="text" bindconfirm="onSearch" bindinput="onInput" value="{{input_code}}"/><block wx:if="{{show_clear}}"><icon type="clear" size="13" catchtap="clearInput"></icon></block></view><text bindtap="cancel">取消</text></view></block><scroll-view class="content" style="{{'height:'+(second_height+'px')+';'}}" enable-back-to-top="{{true}}" scroll-y="true" scroll-with-animation="true"><block wx:for="{{renderList}}" wx:for-item="item" wx:for-index="__i0__"><checkbox-group data-item="{{item}}" id="{{item.id}}" data-id="{{item.id}}" data-event-opts="{{[['change',[['checkboxChange',['$event']]]]]}}" bindchange="__e"><view class="tap_mask" data-username="{{item.member||item.owner}}" data-event-opts="{{[['tap',[['into_room',['$event']]]]]}}" bindtap="__e"><view class="address_bottom" data-username="{{item.member||item.owner}}" data-event-opts="{{[['tap',[['into_room',['$event']]]]]}}" bindtap="__e"><image src="../../../static/images/theme2x.png" data-username="{{item.member||item.owner}}" data-event-opts="{{[['tap',[['into_room',['$event']]]]]}}" bindtap="__e"></image><text data-username="{{item.member||item.owner}}" data-event-opts="{{[['tap',[['into_room',['$event']]]]]}}" bindtap="__e">{{item.member||item.owner}}</text><checkbox value="{{item.member||item.owner}}" checked="{{item.checked}}" disabled="{{item.disabled}}"></checkbox></view></view></checkbox-group></block></scroll-view><view class="btnWraper"><button data-event-opts="{{[['tap',[['startConfr',['$event']]]]]}}" class="button" bindtap="__e">{{buttonText}}</button><button data-event-opts="{{[['tap',[['goBack',['$event']]]]]}}" class="button red" bindtap="__e">返回</button></view></view>