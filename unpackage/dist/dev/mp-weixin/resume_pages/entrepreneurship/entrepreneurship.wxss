.entrepreneurship {
  width: 100vw;
}
.entrepreneurship .list_box {
  width: 90%;
  margin: 40rpx auto;
}
.entrepreneurship .list_box .time {
  width: 100%;
  margin: 40rpx 0rpx;
}
.entrepreneurship .list_box .time .time_item {
  width: 43%;
  height: 92rpx;
  border-radius: 20rpx;
  border: 1px solid #BABABA;
  font-size: 28rpx;
  font-weight: 400;
  color: #4F4F4F;
}
.entrepreneurship .list_box .list_title {
  width: 100%;
  font-size: 28rpx;
  font-weight: 400;
  color: #4F4F4F;
}
.entrepreneurship .list_box .list_title .screen_btn {
  box-sizing: border-box;
  padding: 10rpx 35rpx;
  border-radius: 40rpx;
  color: #fff;
  background-color: #157DFA;
}
.entrepreneurship .list_box .list {
  width: 100%;
  box-sizing: border-box;
  padding: 30rpx;
  background: #fff;
  box-shadow: 0px 0px 8px rgba(186, 186, 186, 0.3);
  border-radius: 20rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #4F4F4F;
}
.entrepreneurship .list_box .list .list_text {
  width: 100%;
  margin: 20rpx 0rpx;
}

