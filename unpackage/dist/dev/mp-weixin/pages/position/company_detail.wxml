<view class="companyDetails"><view class="company_box flex-r-b-c f-row jc-sb ai-c"><view class="company_name"><view class="top"></view><view>{{''+data.name+''}}</view></view><image src="{{data.logo_image}}" mode></image></view><view class="company_list flex-r-b-c" style="padding-bottom:30rpx;border-bottom:0px solid #ECECEC;"><view class="list_title flex-r-s-c"><view class="icon"></view><view class="text">公司规模</view></view><view class="money">{{data.scale}}</view></view><view class="company_list"><view class="list_title flex-r-s-c"><view class="icon"></view><view class="text">公司简介</view></view><view class="list_content">{{''+(data.introduce||'')+''}}</view></view><block wx:if="{{data.information}}"><view class="company_list"><view class="list_title flex-r-s-c"><view class="icon"></view><view class="text">企业资讯</view></view><view class="list_content"><u-parse vue-id="56dff9f6-1" content="{{data.information}}" bind:__l="__l"></u-parse></view></view></block><block wx:if="{{data.images}}"><view class="company_list"><view class="list_title flex-r-s-c"><view class="icon"></view><view class="text">公司环境</view></view><view class="swiper-comp"><swiper class="swiper" indicator-dots="{{true}}" indicator-active-color="white" autoplay="{{false}}" interval="{{4000}}" duration="{{800}}" circular="{{true}}"><block wx:for="{{data.images}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="{{['swiper-item',(swiperIndex===index)?'active':'']}}"><image src="{{item}}" mode="scaleToFill" show-menu-by-longpress="{{true}}" data-event-opts="{{[['tap',[['showImg',['$0',index],[[['data.images','',index]]]]]]]}}" bindtap="__e"></image></view></swiper-item></block></swiper></view></view></block><block wx:if="{{data.vedio}}"><view class="company_list"><view class="list_title flex-r-s-c"><view class="icon"></view><view class="text">企业宣传视频</view></view><view class="swiper-comp"><swiper class="swiper" indicator-dots="{{false}}" autoplay="{{false}}" interval="{{4000}}" duration="{{800}}" circular="{{true}}"><block wx:for="{{data.vedio}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="{{['swiper-item',(swiperIndex===index)?'active':'']}}"><video src="{{item}}"></video></view></swiper-item></block></swiper></view></view></block><block wx:if="{{$root.g0!=0}}"><view class="company_list"><view class="list_title flex-r-s-c pb-3"><view class="icon"></view><view class="text">招聘情况</view></view><view><post-card vue-id="56dff9f6-2" list="{{list}}" data-event-opts="{{[['^tapCard',[['tapCard']]]]}}" bind:tapCard="__e" bind:__l="__l"></post-card></view></view></block><view style="width:100%;height:100rpx;"></view></view>